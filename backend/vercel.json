{
    "version": 2,
    "builds": [
        {
            "src": "src/App.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/App.ts"
        }
    ],
    "functions": {
      "api/**/*.ts": {
        "runtime": "nodejs18.x"
      }
    }
}